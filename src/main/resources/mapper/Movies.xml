<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.MoviesMapper"><!--mapper这是连接对象的mapper-->
    <select id="getall" resultType="com.example.demo.entity.Movies"><!--写查询语句的位置,要表明返回对象-->
        select * from movies
    </select>

</mapper>